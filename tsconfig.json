{
	"compilerOptions": {
		"target": "ES2022",
		"lib": [
			"ES2022"
		],
		"module": "Node16",
		"strict": true,
		"alwaysStrict": true,
		"moduleDetection": "force",
		"moduleResolution": "Node16",
		"outDir": "out",
		"skipLibCheck": false,
		"noUnusedLocals": true,
		"sourceMap": true,
		"rootDir": "src",
	}
}