{
	"name": "eval-and-replace",
	"displayName": "Eval and Replace",
	"description": "Execute JavaScript code and replace the code with the result",
	"publisher": "Lebster",
	"version": "2.2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/LebsterFace/eval-and-replace.git"
	},
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"javascript",
		"eval",
		"js",
		"eval-and-replace"
	],
	"activationEvents": [
		"onCommand:eval-and-replace.eval-javascript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "eval-and-replace.eval-javascript",
				"title": "Eval and Replace: JS"
			}
		],
		"keybindings": [
			{
				"command": "eval-and-replace.eval-javascript",
				"key": "ctrl+shift+j"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint src --ext ts"
	},
	"icon": "images/icon128.png",
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"vm2": "^3.9.9"
	}
}
